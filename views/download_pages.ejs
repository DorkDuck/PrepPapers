<!DOCTYPE html>
	<head>
		<link rel="stylesheet" type="text/css" href="/stylesheets/download.css">
	</head>
	<body>
		<header>
  <div class="bars">
    <ul>
      <li class="cor-1"></li>
      <li class="cor-2"></li>
      <li class="cor-3"></li>
      <li class="cor-4"></li>
      <li class="cor-5"></li>
    </ul>
  </div>
  <div class="header-title">
    <%= title %> 
  </div>
</header>

<section class="content-block">
  <h1>Yearwise papers</h1>
  <% for (var i =0 ;i<saal;i++) { %>
  <h2><%= years[i] %></h2>
  <a href="<%= links[i] %>" class="btn paper-raise">View / Download</a>
  <% } %>
</section>

<footer>
  Use a tool like <STRONG>Winzip</strong>/<STRONG>Winrar</strong> to extract the files .
</footer>

  <script type="text/javascript" src="/javascripts/vendors/jquery-2.1.4.min.js"></script>
  <script>
  $('.btn').click(function() {
    var socket = io.connect('http://127.0.0.1:3000');
    socket.on('connect', function () {
      socket.emit('d_increase',{})
    })
  })
  </script>
	</body>
</html>